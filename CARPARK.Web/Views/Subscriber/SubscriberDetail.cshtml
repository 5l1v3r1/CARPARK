@model CARPARK.DTO.EntitisModelViewDTO.AboneViewModel
@{
    ViewBag.Title = "SubscriberDetail";
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}

<style>
    #sub {
        margin-bottom: 0;
    }
</style>
<div id="content-header">
    <div id="breadcrumb">
        <a href="/DashboardPanel" title="Anasayfaya Geri Dön" class="tip-bottom"><i class="icon-home"></i> Anasayfa</a>
        <a href="/SubscriberList" title="Abone Sayfasına Geri Dön" class="tip-bottom"><i class="icon-user"></i> Aboneler</a>
        <a href="#" class="current">Abone Detay</a>
    </div>
</div>


<div class="container-fluid">
    <hr id="sub">
    <div class="row-fluid">
        <div class="span12">
            <div class="widget-box">
                <div class="widget-title">
                    <div class="pull-left">
                        <span class="icon"><i class="icon-th"></i></span>
                        <h5>Abone Detay</h5>
                    </div>
                </div>
                <div class="widget-content nopadding">
                    <div id="order-view-content" class="table-responsive">
                        <ul id="order-view-tabs" class="nav nav-tabs tabs-left">
                            <li class="active"><a href="#order-view-tabs-order-details" data-toggle="tab"><strong>Abone Bilgileri</strong></a></li>
                            <li><a href="#order-view-tabs-payment" data-toggle="tab"><strong>Üyelik Bilgileri</strong></a></li>
                            <li><a href="#order-view-tabs-arac" data-toggle="tab"><strong>Araç Bilgileri</strong></a></li>
                        </ul>
                        <div class="tab-content">
                            <div id="order-view-tabs-order-details" class="tab-pane active">
                                <table class="table table-hover">
                                    <tbody>
                                        <tr>
                                            <td class="span3">Abone ID:</td>
                                            <td class="span9">@Model.Abone.AboneID</td>
                                        </tr>
                                        <tr>
                                            <td class="span3">İsim:</td>
                                            <td class="span9">@Model.Abone.Adi</td>
                                        </tr>
                                        <tr>
                                            <td class="span3">Soisim:</td>
                                            <td class="span9">@Model.Abone.Soyad</td>
                                        </tr>
                                        <tr>
                                            <td class="span3">TC Kimlik Numara:</td>
                                            <td class="span9">@Model.Abone.TCNO</td>
                                        </tr>
                                        <tr>
                                            <td class="span3">Telefon:</td>
                                            <td class="span9">@Model.Abone.CepTel</td>
                                        </tr>
                                        <tr>
                                            <td class="span3">Kayıt Tarihi:</td>
                                            <td class="span9">@string.Format("{0:dd/MM/yyyy}", Model.Abone.KayitTarihi)</td>
                                        </tr>

                                    </tbody>
                                </table>
                            </div>
                            <div class="tab-pane" id="order-view-tabs-payment">
                                <table class="table table-hover">
                                    <tbody>
                                        <tr>
                                            <td class="span3">Üye ID:</td>
                                            <td class="span9">@Model.Uye.UyeID</td>
                                        </tr>
                                        <tr>
                                            <td class="span3">Kullanıcı Adı:</td>
                                            <td class="span9">@Model.Uye.KullaniciAdi</td>
                                        </tr>
                                        <tr>
                                            <td class="span3">E-posta:</td>
                                            <td class="span9">@Model.Uye.Eposta</td>
                                        </tr>

                                    </tbody>
                                </table>
                            </div>
                            <div class="tab-pane" id="order-view-tabs-arac">
                                <table class="table table-hover">
                                    <tbody>
                                        <tr>
                                            <td class="span3">Arac ID:</td>
                                            <td class="span9">@Model.Arac.AracID</td>
                                        </tr>
                                        <tr>
                                            <td class="span3">Plaka:</td>
                                            <td class="span9">@Model.Arac.Plaka</td>
                                        </tr>
                                        <tr>
                                            <td class="span3">Marka:</td>
                                            <td class="span9">@Model.Marka.Marka</td>
                                        </tr>
                                        <tr>
                                            <td class="span3">Model:</td>
                                            <td class="span9">@Model.Model.Model</td>
                                        </tr>

                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
